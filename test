#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
:vpi_module "/usr/lib/ivl/v2009.vpi";
S_0x564118a9bca0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x564118aaf340 .scope module, "testtb" "testtb" 3 3;
 .timescale 0 0;
v0x564118ac0220_0 .var "arr_tb", 3 0;
v0x564118ac02e0_0 .net "out_tb", 3 0, v0x564118ac00e0_0;  1 drivers
S_0x564118aaf4d0 .scope module, "tb" "test" 3 9, 4 6 0, S_0x564118aaf340;
 .timescale 0 0;
    .port_info 0 /INPUT 4 "arr";
    .port_info 1 /OUTPUT 4 "out";
v0x564118a750d0_0 .net "arr", 3 0, v0x564118ac0220_0;  1 drivers
v0x564118ac00e0_0 .var "out", 3 0;
    .scope S_0x564118aaf4d0;
T_0 ;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x564118ac00e0_0, 0, 4;
    %end;
    .thread T_0, $init;
    .scope S_0x564118aaf4d0;
T_1 ;
    %delay 1, 0;
    %load/vec4 v0x564118a750d0_0;
    %load/vec4 v0x564118ac00e0_0;
    %xnor;
    %store/vec4 v0x564118ac00e0_0, 0, 4;
    %load/vec4 v0x564118ac00e0_0;
    %dup/vec4;
    %pushi/vec4 15, 0, 4;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %vpi_call/w 4 23 "$display", "oob" {0 0 0};
    %jmp T_1.2;
T_1.0 ;
    %vpi_call/w 4 21 "$display", "boo" {0 0 0};
    %jmp T_1.2;
T_1.2 ;
    %pop/vec4 1;
    %load/vec4 v0x564118ac00e0_0;
    %inv;
    %store/vec4 v0x564118ac00e0_0, 0, 4;
    %jmp T_1;
    .thread T_1;
    .scope S_0x564118aaf340;
T_2 ;
    %pushi/vec4 7, 0, 4;
    %store/vec4 v0x564118ac0220_0, 0, 4;
    %vpi_call/w 3 17 "$monitor", "arr = %b, out = %b", v0x564118a750d0_0, v0x564118ac00e0_0 {0 0 0};
    %vpi_call/w 3 20 "$dumpfile", "testfile.vcd" {0 0 0};
    %vpi_call/w 3 21 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x564118aaf340 {0 0 0};
    %delay 18, 0;
    %vpi_call/w 3 24 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 5;
    "N/A";
    "<interactive>";
    "-";
    "testtb.sv";
    "test.sv";
